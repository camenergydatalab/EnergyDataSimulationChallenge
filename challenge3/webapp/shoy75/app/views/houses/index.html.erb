<h1>Houses</h1>

<%= link_to 'All Energies Data', energies_path %>
<%= link_to 'Temperature Energy Chart', temperature_chart_path %>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>City</th>
    <th>Num of people</th>
    <th>Child</th>
  </tr>
  <%- @houses.each do |house| %>
    <tr>
      <td><%= house.id %></td>
      <td><%= link_to house.full_name, house_path(house.id) %></td>
      <td><%= house.city %></td>
      <td><%= house.num_of_people %></td>
      <td><%= house.has_child %></td>
    </tr>
  <%- end %>
</table>
