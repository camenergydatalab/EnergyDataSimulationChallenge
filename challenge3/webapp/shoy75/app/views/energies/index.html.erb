<h1>Energies</h1>

<%= link_to 'All Houses Data', houses_path %>

<table>
  <tr>
    <th>ID</th>
    <th>Label</th>
    <th>House</th>
    <th>Year</th>
    <th>Month</th>
    <th>Temperature</th>
    <th>Daylight</th>
    <th>Energy Production</th>
  </tr>
  <%- @energies.each do |energy| %>
    <tr>
      <td><%= energy.id %></td>
      <td><%= energy.label %></td>
      <td><%= energy.house_id %></td>
      <td><%= energy.year %></td>
      <td><%= energy.month %></td>
      <td><%= energy.temperature %></td>
      <td><%= energy.daylight %></td>
      <td><%= energy.energy_production %></td>
    </tr>
  <%- end %>
</table>
